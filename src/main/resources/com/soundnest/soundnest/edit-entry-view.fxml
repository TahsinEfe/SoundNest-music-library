<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soundnest.soundnest.Controllers.EditEntryController">
    <children>
        <Label layoutX="50.0" layoutY="30.0" text="Edit Entry" style="-fx-font-size: 18px; -fx-font-weight: bold;" />

        <Label layoutX="50.0" layoutY="80.0" text="ID:" />
        <TextField fx:id="inputId" layoutX="150.0" layoutY="75.0" prefWidth="200.0" editable="false" />

        <Label layoutX="50.0" layoutY="130.0" text="Name:" />
        <TextField fx:id="inputName" layoutX="150.0" layoutY="125.0" prefWidth="200.0" />

        <Label layoutX="50.0" layoutY="180.0" text="Password:" />
        <TextField fx:id="inputPassword" layoutX="150.0" layoutY="175.0" prefWidth="200.0" />

        <Button fx:id="saveButton" layoutX="150.0" layoutY="230.0" prefWidth="100.0" text="SAVE" onMouseClicked="#onSaveClick" />
    </children>
</AnchorPane>
